        "overrides": [
          {
            "matcher": {"id": "byRegexp", "options": ".*pass.*"},
            "properties": [
              {"id": "color", "value": {"fixedColor": "green", "mode": "fixed"}}
            ]
          },
          {
            "matcher": {"id": "byRegexp", "options": ".*fail.*"},
            "properties": [
              {"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}}
            ]
          }
        ]
