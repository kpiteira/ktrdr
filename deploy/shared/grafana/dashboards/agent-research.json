      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "(sum(agent_cycles_total{outcome=\"completed\"}) or vector(0)) / clamp_min(sum(agent_cycles_total), 1) * 100",
          "refId": "A"
        }
