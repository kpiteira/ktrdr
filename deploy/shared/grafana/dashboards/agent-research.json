{
  "uid": "agent-research",
  "title": "Agent Research Cycles",
  "description": "Monitoring for autonomous strategy designer research cycles",
  "schemaVersion": 39,
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "tags": ["ktrdr", "agent", "research", "monitoring"],
